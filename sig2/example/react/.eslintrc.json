{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "quotes": "off",
    "import/prefer-default-export": 0,
    "object-curly-newline": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prefer-stateless-function": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "it": true
  }
}
